{
	"name": "fecr",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"check": "run-p -s lint ts test",
		"pretty": "pretty-quick --write --branch master",
		"pretty-all": "prettier \"{data,src,test,tools}/**/*.{js,jsx,json,ts,tsx}\" --write",
		"lint": "eslint --cache \"{data,src,test,tools}/**/*.{js,jsx,json,ts,tsx}\"",
		"ts": "tsc --noEmit",
		"test": "NODE_NO_WARNINGS=1 ava",
		"build": "webpack",
		"watch": "webpack -w",
		"build-prod": "NODE_ENV=production webpack"
	},
	"ava": {
		"compileEnhancements": false,
		"extensions": [
			"ts",
			"tsx"
		],
		"require": [
			"ts-node/register/transpile-only"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"dependencies": {
		"grommet": "^2.14.0",
		"grommet-icons": "^4.4.0",
		"jsonpack": "^1.1.5",
		"lodash": "^4.17.19",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-flip-move": "^3.0.4",
		"styled-components": "^5.1.1"
	},
	"devDependencies": {
		"@babel/core": "^7.10.3",
		"@babel/preset-env": "^7.10.3",
		"@babel/preset-react": "^7.10.1",
		"@babel/preset-typescript": "^7.10.1",
		"@babel/register": "^7.10.3",
		"@types/bluebird": "^3.5.32",
		"@types/js-yaml": "^3.12.5",
		"@types/jsonpack": "^1.1.0",
		"@types/lodash": "^4.14.157",
		"@types/mkdirp": "^0.5.2",
		"@types/react": "^16.9.41",
		"@types/react-dom": "^16.9.8",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"ava": "^2.3.0",
		"babel-loader": "^8.1.0",
		"babel-plugin-inline-import": "^3.0.0",
		"bluebird": "^3.5.5",
		"cache-loader": "^4.1.0",
		"eslint": "^6.8.0",
		"eslint-plugin-import": "^2.22.0",
		"http-server": "^0.12.3",
		"husky": "^3.0.5",
		"js-yaml": "^3.14.0",
		"mkdirp": "^0.5.5",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.18.2",
		"pretty-quick": "^1.11.0",
		"request": "^2.88.2",
		"ts-loader": "^6.2.2",
		"ts-node": "^8.10.2",
		"ts-node-dev": "^1.0.0-pre.49",
		"typescript": "^3.9.5",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12",
		"yargs": "^15.3.1"
	}
}
