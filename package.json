{
	"name": "fe-char-ts",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"check": "run-p -s lint ts test",
		"pretty": "pretty-quick --write --branch master",
		"pretty-all": "prettier \"{src,test}/**/*.{js,jsx,json,css,ts,tsx}\" --write",
		"lint": "eslint --cache \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
		"ts": "tsc --noEmit",
		"test": "NODE_NO_WARNINGS=1 ava",
		"build": "webpack",
		"watch": "webpack -w",
		"build-prod": "NODE_ENV=production webpack",
		"start": "http-server public -c-1 -o index.html"
	},
	"ava": {
		"compileEnhancements": false,
		"extensions": [
			"ts",
			"tsx"
		],
		"require": [
			"ts-node/register/transpile-only"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"dependencies": {
		"grommet": "^2.8.0",
		"grommet-icons": "^4.4.0",
		"jsonpack": "^1.1.5",
		"lodash": "^4.17.11",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-flip-move": "^3.0.4",
		"styled-components": "^4.4.1"
	},
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-react": "^7.7.4",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/register": "^7.4.4",
		"@types/bluebird": "^3.5.27",
		"@types/js-yaml": "^3.12.1",
		"@types/jsonpack": "^1.1.0",
		"@types/lodash": "^4.14.134",
		"@types/mkdirp": "^0.5.2",
		"@types/react": "^16.9.12",
		"@types/react-dom": "^16.9.4",
		"@typescript-eslint/eslint-plugin": "^2.13.0",
		"@typescript-eslint/parser": "^2.13.0",
		"ava": "^2.3.0",
		"babel-loader": "^8.0.6",
		"babel-plugin-inline-import": "^3.0.0",
		"bluebird": "^3.5.5",
		"cache-loader": "^4.1.0",
		"eslint": "^6.8.0",
		"eslint-plugin-import": "^2.19.1",
		"http-server": "^0.12.0",
		"husky": "^3.0.5",
		"js-yaml": "^3.13.1",
		"mkdirp": "^0.5.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.18.2",
		"pretty-quick": "^1.11.0",
		"request": "^2.88.0",
		"ts-loader": "^6.2.1",
		"ts-node": "^8.5.4",
		"ts-node-dev": "^1.0.0-pre.44",
		"typescript": "^3.7.4",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10"
	}
}
