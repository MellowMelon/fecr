{
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"jsx": "react",
		"sourceMap": true,
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"noUnusedLocals": true,
		"baseUrl": ".",
		"skipLibCheck": true,
		"resolveJsonModule": true
	},
	"include": ["src", "test"],
	"exclude": ["data"]
}
